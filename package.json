{
  "name": "@tech-notes/web-components",
  "version": "0.0.1",
  "description": "Tech Notes Web Components",
  "main": "index.js",
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -s ./public -p 9001",
    "build:storybook": "build-storybook -c .storybook -o .out",
    "build:lib": "ts-node --project tools/tsconfig.json tools/build",
    "test": "jest",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "types": "tsc --noEmit",
    "publish:storybook": "npm run build:storybook && ts-node --project tools/tsconfig.json ./tools/publish-showcase",
    "publish:lib:beta": "ts-node --project tools/tsconfig.json tools/publish-npm-beta",
    "publish:lib:major": "ts-node --project tools/tsconfig.json tools/publish-npm-major",
    "publish:lib:minor": "ts-node --project tools/tsconfig.json tools/publish-npm-minor",
    "publish:lib:patch": "ts-node --project tools/tsconfig.json tools/publish-npm-patch",
    "develop:new:component": "ts-node --project tools/tsconfig.json tools/develop-new-component"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tech-notes/components-web.git"
  },
  "keywords": [
    "web",
    "components",
    "library"
  ],
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.10",
    "@storybook/addon-docs": "^5.3.10",
    "@storybook/addon-knobs": "^5.3.10",
    "@storybook/react": "^5.3.10",
    "@testing-library/react": "^9.4.0",
    "@types/faker": "^4.1.9",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "inquirer": "^7.0.4",
    "jest": "^25.1.0",
    "jest-html-reporter": "^3.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "rimraf": "^3.0.1",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "engineStrict": true,
  "engines": {
    "node": "12.16.2",
    "npm": "6.14.4"
  }
}
